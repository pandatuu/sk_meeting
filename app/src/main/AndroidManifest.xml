<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    package="com.example.facetime">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!--获取手机型号等相关信息-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <!--获取地址相关信息-->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/send_photo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/send_photo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"

        android:resizeableActivity="false"
        >
        <activity
            android:name=".conference.MenuActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.mainPageActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <!-- sk://appShare?参数1=xxx&参数2=xxx -->
                <data android:scheme="sk" android:host="appShare" tools:ignore="AppLinkUrlError"/>
            </intent-filter>
        </activity>

        <!-- 首页 -->
        <activity android:name=".login.MainActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>


        <!-- 国家区号 -->
        <activity android:name="com.sahooz.library.PickActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!-- 注册 -->
        <activity android:name="com.example.facetime.conference.RegisterActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!-- 注册-设置密码 -->
        <activity android:name="com.example.facetime.conference.RegisterSetPassword"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!-- 设置昵称 -->
        <activity android:name="com.example.facetime.conference.RegisterSetNickName"
android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!-- 用户协议 -->
        <activity android:name="com.example.facetime.login.UserAgreement"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!-- 服务声明 -->
        <activity android:name="com.example.facetime.login.ServiceStatement"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>


        <!--选择页-->
        <activity android:name="com.example.facetime.conference.BaseActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>


        <!--设置,修改密码,昵称-->
        <activity android:name="com.example.facetime.conference.SystemSettingsActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!--登录页面-->
        <activity android:name="com.example.facetime.login.StartActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!--准备更改密码-->
        <activity android:name="com.example.facetime.login.ReadSetPasswordActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!--更改密码-->
        <activity android:name="com.example.facetime.login.RsetPasswordActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>


        <!--聊天视频-->
        <activity android:name="com.example.facetime.conference.JitsiMeetActivitySon"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>


        <!--设置-->
        <activity android:name="com.example.facetime.conference.SettingsActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>


        <!--设置密码-->
        <activity android:name="com.example.facetime.conference.CreatePasswordActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>


        <!--设置房间名-->
        <activity android:name="com.example.facetime.conference.CreateRoomNameActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!--创建房间成功-->
        <activity android:name="com.example.facetime.conference.SuccessActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!--通过房间号进入-->
        <activity android:name="com.example.facetime.conference.EnteRoomByIdActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!--通过密码进入房间-->
        <activity android:name="com.example.facetime.conference.EnteRoomByPasswordActivity"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>


        <!--修改密码-->
        <activity android:name="com.example.facetime.conference.UpdatePassword"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>

        <!--修改昵称-->
        <activity android:name="com.example.facetime.conference.UpdateNickName"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme.Black"
            >
        </activity>


    </application>

</manifest>